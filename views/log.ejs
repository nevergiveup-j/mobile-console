<% include ./components/header %>

<div class="wrap">
  <div class="log-header">
    <div class="fn-right">
      <a href="javascript:" class="button-mark">MARK</a>
    </div>
  </div>
  <ul class="log-list" id="J_logList">
    <li style="display: none;">
      <div class="mark-line">
        <span class="mark-date">[MARKED 15:27:22]</span>
      </div>
    </li>
  </ul>
</div>

<script id="log-list-tpl" type="text/template">
  <@ _.each(datas, function(item) { @>
    <li>
      <span class="gray">
        <span class="date">[<@=item.date@>]</span>
        <span class="browser"><@=item.bowser@> <@=item.version@> (<@=item.platform@>)</span>
      </span>
      <span class="level">[<@=item.type@>]</span>
      <span class="args"><@=item.body@></span>
    </li>
  <@ }) @>
</script>

<% include ./components/footer %>

<script src="/static/js/log/socket.io-1.4.8.js"></script>
<script src="/static/js/log/underscore.js"></script>
<script src="/static/js/log/log.js"></script>
